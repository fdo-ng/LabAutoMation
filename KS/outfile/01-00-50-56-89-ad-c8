  KERNEL /Images/ESXi6.0u2/mboot.c32
   APPEND -c /pxelinux.cfg/boot/01-00-50-56-89-ad-c8boot.cfg ks=http://10.10.10.5/KS/01-00-50-56-89-ad-c8.cfg +++
MENU LABEL ESXi-=ESXi-6.0.2-3620759-full Installer
LABEL hddboot
